#include <bits/stdc++.h>
using namespace std;

int main(){
    int i,j;
    for(i=0;i<=5;i++){
        for(j=0;j<i;j++){
            cout<<" ";
        }
        for(j=0;j<2*5-2*i-1;j++){
            cout<<"*";
        }
        for(j=0;j<i;j++){
            cout<<" ";
        }cout<<endl;
    }
        
}
