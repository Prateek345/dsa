#include <iostream>
using namespace std;

int main(){
    int i,j,star=1;
    for(i=0;i<5;i++){
        if(star%2==0){
            cout<<"1";
        }
        else{
            cout<<"0";
        }
        for(j=0;j<i;j++){
            cout<<star;
            star=1-star;
            
        }cout<<endl;
    }
}
